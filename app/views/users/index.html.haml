- content_for :title do
  Community

= image_tag "plus/body_com.jpg", :class => "body_header"
.alpha.grid24.body_about
  %span{:class => "p_a"}
    Community
  %span{:class => "p_a1", :style => "position: relative !important; left: -169px;"}
    = link_to "Home", "/"
    > Community
  %ul{:class => "Fright1"}
    %li
      = link_to "Week", "#", :class => "selected1"
    %li
      = link_to "Month", "#"
    %li
      = link_to "Quarter", "#"
    %li
      = link_to "Year", "#"
    %li
      = link_to "All", "#"
  .tabs
    %ul{:class => "tabNavigation"}
      %li
        = link_to "Reputation", "#first", :class => "selected"
      %li
        = link_to "New Users", "#second"
      %li
        = link_to "Oldest", "#third"
      %li
        = link_to "Name", "#foure"
      %li
        = link_to "Nearby", "#five"
    .clear
    = form_tag "/users", :id => "search1", :method => "get" do
      = text_field_tag "q", params[:q]
      = link_to image_tag("plus/serch12.png", :class => "img12_1"), "#", :id => "search_user_link"
      .clear
    .first{:style => "display:block"}
      %ul{:class => "slider22", :style => "position:relative;"}
        %li{:class => "pager", :style => "width: 960px; overflow: hidden; list-style: none; outside: none;color:#000;"}
          %ul
            -@memberships.each do |membership|
              = render "users/plus_membership", :membership => membership